<style>
    .custom-spinner{
        display: none;
    }
    .special-class .custom-spinner{
        display: inline-block;
    }
    .special-class .custom-finger-emoji{
        display: none;
    }
.custom-finger-emoji{
  -webkit-animation: myAnim 2s ease infinite;
          animation: myAnim 2s ease infinite;
  margin-top: -2px;
}

@-webkit-keyframes myAnim {
  0% {
    transform: translateX(0) rotate(-5deg);
  }
  50% {
    transform: translateX(10px) rotate(8deg);
  }
  100% {
    transform: translateX(0) rotate(-5deg);
  }
}

@keyframes myAnim {
  0% {
    transform: translateX(0) rotate(-5deg);
  }
  50% {
    transform: translateX(10px) rotate(8deg);
  }
  100% {
    transform: translateX(0) rotate(-5deg);
  }
}

