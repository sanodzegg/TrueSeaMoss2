    }
}
</style>

<script>
    function clearCartAndAddNewItem(variantId, sellingPlanId, customAttributes, button, includeFreeGift) {
        // Add a special class to the button
        $(button).addClass('special-class');

        $(button).find('.text-xl').text('Loading...');
    
        // Disable the button to prevent multiple clicks
        $(button).prop('disabled', true);

        // First, clear the cart
        $.ajax({
            type: 'POST',
            url: '/cart/clear.js',
            dataType: 'json',
            contentType: 'application/json',
            success: function() {
                // After clearing, add the new item
                addToCartAndRedirect(variantId, sellingPlanId, customAttributes, button, includeFreeGift);
            },
            error: function() {
                alert('Could not clear the cart. Please try again.');
                $(button).prop('disabled', false); // Re-enable the button if cart clearing fails
            }
        });
    }
    
    function addToCartAndRedirect(variantId, sellingPlanId, customAttributes, button, includeFreeGift) {
        var itemsToAdd = [{
            id: variantId,
            quantity: 1,
            properties: customAttributes
        }];
    
        if (sellingPlanId) {
            itemsToAdd[0].selling_plan = sellingPlanId;
        }

        // If free gift should be included, add it to the items
        if (includeFreeGift) {
            var freeGiftVariantId = 43590211076251; // Replace with your free gift's variant ID
            itemsToAdd.push({
                id: freeGiftVariantId,
                quantity: 1
            });
        }

        console.log(includeFreeGift, itemsToAdd)

        var data = {
            items: itemsToAdd
        };
    
        $.ajax({
            type: 'POST',
            url: '/cart/add.js',
            data: JSON.stringify(data),
            dataType: 'json',
            contentType: 'application/json',
            success: function() {
                window.location.href = '/checkout';
            },
            error: function() {
                alert('Could not add product to cart. Please try again.');
                $(button).prop('disabled', false); // Re-enable the button in case of error
            }
        });
    }
    
    document.addEventListener('DOMContentLoaded', function() {
        $('#custom-atc-1').on('click', function() {
            // Include free gift for custom-atc-1
            clearCartAndAddNewItem(43457591345307, 1634631835, { 'Taste 1': 'Pineapple & Mango - 1', 'Taste 2': 'Elderberry - 1' }, this, true);
        });
    
        $('#custom-atc-2').on('click', function() {
            // No free gift for custom-atc-2
            clearCartAndAddNewItem(43457591115931, null, { 'Taste 1': 'Elderberry - 1' }, this, false);
        });
    });
    
    
</script>

